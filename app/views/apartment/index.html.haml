%button.btn.btn-outline-primary.float-right
  = link_to "Refresh listings", refresh_path, :method => :get
%h1 #{@apartments.length} Currently Available Listings: 
%br
%br
%table
  %tr
    -@apartments.each do |a|
      %p= "#{a.name} --- ##{a.unit_num} --- #{a.style} --- #{a.price} --- #{a.sq_ft} --- #{a.bed_bath} --- #{a.availability} --- #{a.link}"
